{
  "manifest_version": 2,

  "name": "QR code",
  "description": "Translates your URL into QR code.",
  "version": "1.0",
  "icons" : {"16": "icon.png", "48": "icon48.png"},

  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
